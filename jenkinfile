
pipeline  {
    agent any
    stages {
        stage('build') {
            steps  {
            echo "building the project ........."
            }
}
        stage('deploy') {
            steps {
            echo "deploy the project ........."
  }
}
        stage('test') {
            steps {
            echo "testing  the project ........."
  }
}
        stage('release') {
            steps  {
            echo "release  the project ........."
      }
   }
  }
}
